   "source": [
    "# initialize columns in data frame for photometry results\n",
    "df[[\"ch1flux\",\"ch1flux_unc\",\"ch2flux\",\"ch2flux_unc\",\"ch3flux\",\"ch3flux_unc\",\"ch4flux\",\"ch4flux_unc\",\"ch5flux\",\"ch5flux_unc\",\"ch6flux\",\"ch6flux_unc\"]] = 0.0\n"
